{
  "name": "process-flow-configurator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=23.x.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "dev": "react-router dev",
    "build": "pnpm typegen && react-router build",
    "serve": "vite preview",
    "biome:check": "biome check",
    "biome:fix": "pnpm biome:check --write",
    "biome:ci": "biome ci",
    "typegen": "react-router typegen",
    "type:check": "react-router typegen && tsc",
    "turbo": "pnpm dlx turbo@2.1.2",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage run",
    "check:turbo": "pnpm turbo biome:check type:check test",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky",
    "knip": "knip"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@react-router/node": "^7.2.0",
    "@types/antd": "^1.0.4",
    "@xyflow/react": "^12.4.4",
    "antd": "^5.24.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "isbot": "^5",
    "lucide-react": "^0.477.0",
    "pnpm": "^10.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/config-conventional": "^19.7.1",
    "@react-router/dev": "^7.2.0",
    "@types/node": "^22.13.8",
    "@types/react": "19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.7",
    "@vitest/ui": "^3.0.7",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "knip": "^5.45.0",
    "lint-staged": "^15.4.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.7"
  }
}
